{
  "name": "react-video",
  "version": "1.0.0",
  "description": "A modern, responsive React video player component with customizable controls, thumbnail previews, and multiple viewing modes",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/react-video.umd.cjs",
  "module": "./dist/react-video.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/react-video.js",
      "require": "./dist/react-video.umd.cjs"
    },
    "./style.css": "./dist/index.css"
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "prepare": "npm run build"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.15",
    "@types/node": "^22.13.11",
    "@types/react": "^18.2.40",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.3.4",
    "tailwindcss": "^4.0.15",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3"
  },
  "dependencies": {
    "path": "^0.12.7",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  },
  "keywords": [
    "react",
    "video",
    "player",
    "video-player",
    "react-component",
    "thumbnails",
    "theater-mode",
    "fullscreen",
    "picture-in-picture"
  ],
  "author": "React Video Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZiyaAkhundov/react-video.git"
  },
  "bugs": {
    "url": "https://github.com/ZiyaAkhundov/react-video/issues"
  },
  "homepage": "https://github.com/ZiyaAkhundov/react-video"
}
